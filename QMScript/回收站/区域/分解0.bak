Sub beibao
	Delay 1000*40
	While True
		FindPic 778,590,804,602,"C:\hehe\背包打开.bmp",0.6,intX,intY
		If intX > 0 And intY > 0 Then 
			Call zizhuangbei
		Else 
			MoveTo 879,722
			LeftClick 1
			Delay 1000
		End If
	Wend
End Sub

Sub zizhuangbei
	While True
		MoveTo 360,180
		FindColorEx 370,192,824,535,"E23AA0",0,0.9,intX,intY
		If intX > 0 And intY > 0 Then 
			FindColorEx intX-15, intY-15, intX+15, intY+15, "535A6B", 0, 0.9, intX1, intY1
			If intX>0 and intY > 0 Then
		 		MoveTo intX, intY
